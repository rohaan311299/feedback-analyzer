{
	"name": "feedback-analyzer",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "wrangler dev --remote false",
		"deploy": "wrangler deploy",
		"d1:create": "wrangler d1 create feedback_db",
		"d1:init-local": "wrangler d1 execute feedback_db --local --file=./src/schema.sql",
		"d1:init-remote": "wrangler d1 execute feedback_db --remote --file=./src/schema.sql",
		"tail": "wrangler tail"
	},	  
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.8.19",
		"typescript": "^5.5.2",
		"vitest": "~3.2.0",
		"wrangler": "^4.59.2"
	}
}